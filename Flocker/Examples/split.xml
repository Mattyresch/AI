<?xml version="1.0"?>
<!-- Matthew Resch, Intro to Artificial Intelligence -->
<!-- Test file: Flocker Project, Case 1 -->
<!-- A group of flockers go around an obstacle to reach a light source -->

<world xmlns="http://perceptualscience.rutgers.edu/flocking"
   width="500" height="500" time="100" debug="true">
<defaults>
   <flocker 
         evade="true" debug="false" clear="true" align="false" center="true"/>
</defaults>
   <!-- Flockers -->
   <flocker x="100" y="40" heading="0" />
   <flocker x="100" y="80" heading="0" />
   <flocker x="100" y="120" heading="0" />
   <!-- Rocks/obstacles -->
   <rock x="200" y="80"/>
   <rock x="200" y="90"/>
   <rock x="200" y="100"/>
   <rock x="200" y="110"/>
   <rock x="200" y="120"/>
   <rock x="400" y="70"/>
   <rock x="400" y="80"/>
   <rock x="400" y="90"/>
   <rock x="400" y="100"/>
   <rock x="400" y="110"/>
   <rock x="400" y="120"/>
   <rock x="400" y="130"/>
   <rock x="400" y="140"/>
   <rock x="400" y="150"/>
   <!-- Light source -->
   <light x="350" y="100">
</world>

